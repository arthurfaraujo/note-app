<!DOCTYPE html>
<html lang="pt-br">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/noteList.css" />
    <script src="https://code.iconify.design/iconify-icon/1.0.8/iconify-icon.min.js"></script>
    <title>Noteify</title>
  </head>

  <body>
    <%- include('partials/navBar') %>
    <main class="container">
      <div class="createNote" contenteditable="true">
        <div class="createNoteTitle" style="display: none" spellcheck="false">
          Note title
        </div>
        <div
          class="createNoteContent"
          contenteditable="false"
          spellcheck="false"
        >
          Create a note...
        </div>
      </div>
      <div id="root" class="noteList"></div>
    </main>
    <script type="module" src="js/noteList.js"></script>
  </body>
</html>
