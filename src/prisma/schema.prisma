generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  userNickname String  @id
  userEmail    String? @unique
  userPassword String
  userName     String
  userNotes    note[]
}

model note {
  noteId       Int    @id
  noteContent  String
  noteType     Int
  userNickname String
  user         user   @relation(fields: [userNickname], references: [userNickname])
}
